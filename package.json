{
	"name": "umaxica-edge",
	"type": "module",
	"scripts": {
		"dev": "bun run --hot app/server.tsx",
		"dev:com": "bunx wrangler dev -c com/wrangler.jsonc --port 4001",
		"dev:app": "bunx wrangler dev -c app/wrangler.jsonc --port 4002",
		"dev:org": "bunx wrangler dev -c org/wrangler.jsonc --port 4003",
		"deploy:com": "bunx wrangler deploy -c com/wrangler.jsonc",
		"deploy:app": "bunx wrangler deploy -c app/wrangler.jsonc",
		"deploy:org": "bunx wrangler deploy -c org/wrangler.jsonc",
		"build": "vite build && cp -r public/* dist/",
		"preview": "vite preview",
		"deploy": "bun run build && bunx wrangler deploy",
		"cf-typegen": "bunx wrangler types --env-interface CloudflareBindings",
		"format": "bunx @biomejs/biome format . --write",
		"lint": "bunx @biomejs/biome lint . --fix",
		"typecheck": "bunx tsc --noEmit",
		"test": "bun test"
	},
	"dependencies": {
		"@types/react-router": "^5.1.20",
		"hono": "^4.9.0",
		"honox": "^0.1.44",
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"react-router": "^7.8.0",
		"react-router-dom": "^7.8.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.1.4",
		"@cloudflare/workers-types": "^4.20250812.0",
		"@testing-library/react": "^16.3.0",
		"@types/bun": "^1.2.20",
		"@types/react": "^19.1.10",
		"@types/react-dom": "^19.1.7",
		"@typescript/native-preview": "^7.0.0-dev.20250811.1",
		"happy-dom": "^18.0.1",
		"typescript": "^5.9.2",
		"wrangler": "^4.28.1",
		"@cloudflare/vite-plugin": "^1.11.2",
		"vite": "^7.1.2",
		"vite-ssr-components": "^0.5.0"
	},
	"module": "app/server.tsx",
	"private": true
}
