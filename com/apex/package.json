{
  "name": "umaxica-apps-edge-com-apex",
  "type": "module",
  "scripts": {
    "server": "vite",
    "build": "vite build && node --experimental-strip-types ./scripts/export-spa-routes.ts",
    "preview": "$npm_execpath run build && vite preview",
    "deploy": "$npm_execpath run build && wrangler deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings"
  },
  "dependencies": {
    "hono": "^4.12.1"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.25.2",
    "vite": "^6.3.5",
    "wrangler": "^4.67.0"
  }
}
