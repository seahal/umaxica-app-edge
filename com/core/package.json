{
  "name": "umaxica-apps-edge-com-core",
  "version": "0.0.1",
  "private": true,
  "description": "code for xx.umaxica.com",
  "type": "module",
  "scripts": {
    "build": "react-router build && node --experimental-strip-types ./scripts/export-spa-routes.ts",
    "typegen": "react-router typegen",
    "cf-typegen": "pnpm exec wrangler types",
    "deploy": "pnpm run build && pnpm exec wrangler deploy",
    "server": "react-router dev",
    "postinstall": "pnpm run cf-typegen || true",
    "preview": "pnpm run build && vite preview",
    "type": "pnpm exec tsgo --noEmit"
  },
  "dependencies": {
    "@sentry/react-router": "^10.39.0",
    "i18next": "^25.8.10",
    "isbot": "^5.1.35",
    "react": "^19.2.4",
    "react-aria-components": "^1.15.1",
    "react-dom": "^19.2.4",
    "react-i18next": "^16.5.4",
    "react-router": "^7.13.0",
    "react-router-dom": "^7.13.0",
    "remix-i18next": "^7.4.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260228.0",
    "@react-router/dev": "^7.13.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "happy-dom": "^20.6.1",
    "vite": "^6.3.5",
    "vitest": "^4.0.18",
    "wrangler": "^4.67.0"
  }
}
